#include <bits/stdc++.h>

using namespace std;

int main()
{
    string palabra1, palabra2, palabra3, aux;
    cin >> palabra1;
    cin >> palabra2;
    cin >> palabra3;
    std::string resultado = palabra1 + palabra2 + palabra3;
    int res=resultado.size();
    for(int i=0;i<res;i++)
    {
        for(int j=0;j<res;j++)
            if (resultado[j]>resultado[j+1])
        {
            aux[0]=resultado[j];
            resultado[j]=resultado[j+1];
            resultado[j+1]=aux[0];
        }
    }
    float res1=(res/2)+1;
    for (int i=res1; i<=res1; i++){
        cout << resultado[i];
    }
    return 0;
}
