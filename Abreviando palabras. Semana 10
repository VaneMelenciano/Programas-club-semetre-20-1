	#include <bits/stdc++.h>

	using namespace std;

	int main()
	{
    		string palabra1, palabra2, palabra3, aux;
		cin >> palabra1;
		cin >> palabra2;
    		cin >> palabra3;
    		std::string resultado = palabra1 + palabra2 + palabra3;
		sort(resultado.begin(), resultado.end());
		int res1=(resultado.size())/2;
		cout << resultado[res1];
		return 0;
	}
